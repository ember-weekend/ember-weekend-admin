<div class="col-md-4 col-xs-4">
  <div class="panel panel-default" data-test-show-note-new>
    <div class="panel-heading clearfix">
      <h3 class="panel-title">
        Show Note
      </h3>
    </div>
    <div class="panel-body">
      {{show-note-form
        showNote=model
        selectResource=(route-action 'selectResource' model.episode model)
        newResource=(route-action 'newResource' model.episode model)
        save=(route-action 'save')
        cancel=(transition-to 'episodes.show' model.episode)
      }}
    </div>
  </div>

  {{outlet}}

</div>
