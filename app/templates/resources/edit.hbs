<div class="col-md-4 col-xs-4">
  <div class="panel panel-default" data-test-resource-edit>
    <div class="panel-heading clearfix">
      <h3 class="panel-title">
        Resource
      </h3>
    </div>
    <div class="panel-body">
      {{resource-form
        resource=model
        save=(route-action 'save')
        cancel=(transition-to 'resources.show' model)}}
    </div>
  </div>
</div>
