<div class="panel panel-default" data-test-show-note>
  <div class="panel-heading clearfix">
    <h3 class="panel-title" data-test-show-note-title>
      Show Note
    </h3>
  </div>
  <div class="panel-body">
    <dl>
      <dt>
        <label>Time Stamp</label>
      </dt>
      <dd data-test-episode-description>
        {{showNote.timeStamp}}
      </dd>
    </dl>

    <div class="form-actions">
      <button class="btn btn-default pull-right"
        {{action (transition-to 'episodes.show.show-note.edit' showNote.episode showNote)}}
        data-test-edit-show-note-btn>
        Edit
      </button>
    </div>

  </div>
</div>

